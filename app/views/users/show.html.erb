<br>    
<div class="forms">
    <h1><%= @user.name %></h1>
    <% if @user.image.present? %>
        <%= cl_image_tag(@user.image, :style => 'object-fit: cover; border-radius: 50%; width: 200px; height: 200px;') %>
    <% else %> 
        <%= image_tag('https://cdn-icons-png.flaticon.com/512/1053/1053244.png?w=360', :style => 'object-fit: cover; border-radius: 50%; width: 200px; height: 200px;') %>
    <% end%>
</div>
<br><br>
<%  @user.songs.reverse.each do |u|%>
    <div class="songs">
        <h1><%= u.title  if u.present?%></h1>
        <% if u.title.present? %>
            <%= cl_image_tag(u.image, :class => 'feedArt') if u.image.present? %>
            <% if u.present? && u.sound.present?%>
                <iframe id="sc-widget" width="100%" height="166" scrolling="no" frameborder="no" src="https://w.soundcloud.com/player/?url=<%= u.sound %>&show_artwork=true"></iframe>
            <% end %>
            <h2><%= u.bpm%> BPM</h2>
            <% u.genres.each  do |genre|%>
            #<%= genre.name %>
            <% end %>
    </div>
        <% end%>
<% end %>
